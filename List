import java.awt.*;
public class ListExample
{
 ListExample(){
 Frame f= new Frame();
 List l1=new List(5);
 l1.setBounds(100,100, 75,75);
 l1.add("Orange");
 l1.add("Banana");
 l1.add("Mango");
 l1.add("Apple");
 l1.add("Grapes");
 f.add(l1);
 f.setSize(400,400);
 f.setLayout(null);
 f.setVisible(true);
 }
 public static void main(String args[])
{
 new ListExample();
}}
